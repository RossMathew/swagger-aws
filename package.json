{
  "name": "swagger-aws",
  "version": "2.2.1000-pre",
  "description": "Swagger and SwaggerUI extensions for AWS4 authorization support",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "e2e": "npm-run-all build nightwatch",
    "flow": "flow",
    "jest": "jest",
    "lint": "eslint .",
    "nightwatch": "nightwatch -e chrome,default",
    "serve": "node -e 'require(\"./test/server.js\").listen(9000)'",
    "start": "npm-run-all build serve",
    "test": "npm-run-all --parallel lint jest flow"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-export-extensions",
      "transform-flow-strip-types"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "jest"
    ],
    "env": {
      "browser": true,
      "jest/globals": true
    }
  },
  "jest": {
    "browser": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**.js"
    ],
    "moduleNameMapper": {
      "^b$": "empty-module",
      "^handlebars/runtime$": "swagger-ui/dist/lib/handlebars-4.0.5",
      "^sanitize-html$": "swagger-ui/dist/lib/sanitize-html.min"
    },
    "setupFiles": [
      "./test/setupGlobalEnv.js"
    ],
    "transform": {
      "\\.js$": "babel-jest",
      "\\.handlebars": "<rootDir>/test/handlebarsTransformer.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scality/swagger-aws.git"
  },
  "author": "Nicolas Trangez <nicolas.trangez@scality.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Scality/swagger-aws/issues"
  },
  "homepage": "https://github.com/Scality/swagger-aws#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "backbone": "1.1.2",
    "chromedriver": "^2.27.3",
    "empty-module": "0.0.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "flow-bin": "^0.40.0",
    "geckodriver": "^1.4.0",
    "handlebars": "4.0.5",
    "handlebars-loader": "^1.4.0",
    "jest": "^19.0.2",
    "jquery": "^1.8.0",
    "lodash": "3.10.1",
    "nightwatch": "^0.9.12",
    "npm-run-all": "^4.0.2",
    "sanitize-html": "1.11.4",
    "selenium-server-standalone-jar": "^3.1.0",
    "swagger-ui": "2.2.10",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "aws4": "^1.6.0",
    "querystring-browser": "^1.0.4",
    "url": "^0.11.0"
  },
  "peerDependencies": {
    "backbone": "1.1.2",
    "handlebars": "4.0.5",
    "jquery": "1.8.0",
    "lodash": "3.10.1",
    "sanitize-html": "1.11.4",
    "swagger-ui": "2.2.10"
  }
}
