{
  "name": "swagger-ui-aws",
  "version": "0.0.1",
  "description": "Swagger-UI extended with AWS authorization support",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "lint": "better-npm-run lint",
    "clean": "rimraf dist",
    "serve": "node bin/server.js"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "lint": {
      "command": "eslint \"src/**/*.js\" webpack.config.js"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scality/swagger-ui-aws.git"
  },
  "author": "Nicolas Trangez <nicolas.trangez@scality.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Scality/swagger-ui-aws/issues"
  },
  "homepage": "https://github.com/Scality/swagger-ui-aws#readme",
  "dependencies": {
    "aws4": "^1.6.0",
    "backbone": "1.1.2",
    "handlebars": "4.0.5",
    "highlight.js": "9.1.0",
    "jquery": "1.8.2",
    "querystring-browser": "^1.0.4",
    "sanitize-html": "1.11.4",
    "swagger-ui": "2.2.10"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "better-npm-run": "0.0.14",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
